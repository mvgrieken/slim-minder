{
  "name": "@slim-minder/mobile",
  "private": true,
  "version": "0.1.0",
  "main": "index.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build:android": "expo build:android",
    "build:ios": "expo build:ios",
    "start:web": "expo start --web",
    "prebuild:web": "echo 'Skipping dependency validation for faster builds'",
    "build:web": "npx expo export --platform web --output-dir web-build --clear --no-minify && copy public\\_redirects web-build\\_redirects 2>nul || echo 'No _redirects file found'",
    "build:web:verbose": "DEBUG=expo:* npm run build:web",
    "build:web:alt": "npx @expo/cli export --platform web --output-dir web-build --clear --no-minify",
    "preview:web": "npx http-server web-build -p 4173",
    "build": "expo export --platform web --output-dir web-build && cp public/_redirects web-build/_redirects",
    "validate-deps": "node ../../scripts/validate-dependencies.js",
    "typecheck": "tsc --noEmit",
    "eject": "expo eject"
  },
  "dependencies": {
    "@expo/metro-runtime": "~3.1.3",
    "@react-native-async-storage/async-storage": "^1.23.1",
    "@react-navigation/bottom-tabs": "^6.5.20",
    "@react-navigation/native": "^6.1.17",
    "@react-navigation/native-stack": "^6.11.0",
    "@supabase/supabase-js": "^2.45.4",
    "at-least-node": "^1.0.0",
    "date-fns": "^3.3.1",
    "expo": "~52.0.0",
    "expo-asset": "^11.1.7",
    "expo-constants": "~15.4.5",
    "expo-device": "~5.9.3",
    "expo-linking": "~6.2.2",
    "expo-notifications": "^0.31.4",
    "expo-status-bar": "~1.11.1",
    "metro": "^0.80.12",
    "metro-core": "^0.80.12",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.50.1",
    "react-native": "0.74.0",
    "react-native-chart-kit": "^6.12.0",
    "react-native-elements": "^3.4.3",
    "react-native-flash-message": "^0.4.2",
    "react-native-gesture-handler": "~2.14.0",
    "react-native-modal": "^13.0.1",
    "react-native-paper": "^5.12.1",
    "react-native-reanimated": "~3.6.2",
    "react-native-safe-area-context": "^4.10.5",
    "react-native-screens": "^3.31.1",
    "react-native-svg": "14.1.0",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-vector-icons": "^10.0.3",
    "react-native-web": "~0.19.10"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-native": "^0.73.0",
    "@types/react-native-vector-icons": "^6.4.18",
    "expo-build-properties": "~0.11.1",
    "typescript": "^5.4.0"
  },
  "engines": {
    "node": ">=22.0.0",
    "npm": ">=10.0.0"
  },
  "volta": {
    "node": "22.17.1",
    "npm": "10.9.2"
  }
}
